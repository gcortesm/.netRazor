@page  "{restaurantId}"
@model Food.Pages.resta.detailsModel
@{
    ViewData["Title"] = "details";
}

<h2>Detallle de productos</h2>

<div class="container">
    <label>@Model.restaurant.name</label>
    <label>@Model.restaurant.locationn</label>
    <label>@Model.restaurant.type</label>
</div>
@if (Model.message != null)
{
    <label class=" alert alert-success"> @Model.message</label>
}
<a asp-page="./index"> Volver al listado </a>


